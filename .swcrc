{
  "$schema": "https://swc.rs/schema.json",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    }
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false
}